<?xml version="1.0"?>
<registry>

    <records prefix="plone.app.mosaic.app_tiles.news"
        interface="plone.app.mosaic.interfaces.ITile">
        <value key="name">iaweb.mosaic.tile.tiles</value>
        <value key="label">News and Events</value>
        <value key="category">advanced</value>
        <value key="tile_type">app</value>
        <value key="default_value"></value>
        <value key="read_only">false</value>
        <value key="settings">true</value>
        <value key="favorite">false</value>
        <value key="rich_text">false</value>
        <value key="weight">10</value>
    </records>

    <records prefix="plone.resources/iaweb-mosaic"
         interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="css">
          <element>++plone++iaweb.mosaic/slider.ideabox.less</element>
        </value>
	<value key="js">++plone++iaweb.mosaic/slider.iaweb.mosaic.js</value>
    </records>
    <records prefix="plone.resources/iaweb-mosaic-flexslider"
         interface='Products.CMFPlone.interfaces.IResourceRegistry'>
	 <value key="js">++plone++iaweb.mosaic/jquery.flexslider.js</value>
    </records>
    <records prefix="plone.bundles/iaweb-mosaic"
                interface='Products.CMFPlone.interfaces.IBundleRegistry'>
      <value key="resources">
        <element>iaweb-mosaic</element>
        <element>iaweb-mosaic-flexslider</element>
      </value>
      <value key="enabled">True</value>
      <value key="jscompilation">++plone++static/iaweb-mosaic.js</value>
      <value key="csscompilation">++plone++static/iaweb-mosaic.css</value>
      <value key="last_compilation"></value>
    </records>

</registry>
